@model List<finalproject.Models.Tbl_Books>


@foreach (var item in Model)
{
    <table>
        <tr>
            <td><img style="width:50px;height:50px;" src="@Url.Content(item.Book_img)" /></td>
            <td>Book name</td>
            <td> @item.Book_name </td>
            <td>@item.Book_author.auth_name</td>
        
        </tr>
    </table>
}
