@model IEnumerable<finalproject.Models.Reading_History>


<div class="row" style="margin-bottom: 50px;">

    <div class="row" style="margin-bottom: 20%;">


        <div class="row">



            @foreach (var item in Model)
            {

                <div class="col-lg-4 col-md-4" style="height:200px;margin-bottom:50px ;padding-left:30px">

                   



                        <img class="img-rounded img-responsive cimg" style="height:100%; width:100%; " src="@Url.Content(item.Ebooks_db.Ebook_img)" />



                        @*<label>Name </label>*@

                        <p style="text-align:center;font-size:20px">
                            <strong>@item.Ebooks_db.Ebook_name</strong>
                        </p>

                        </div>

                   




             }


            </div>

        </div>


   

    </div>



