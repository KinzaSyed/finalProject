@model IEnumerable<finalproject.Models.BTransaction>

@{
    ViewBag.Title = "Order";
}

<h2>Order</h2>
<table class="table">
    <tr>
        <th>
            Total Bill
        </th>
        <th>
            Book Quantity
        </th>
        <th>
            Order Date
        </th>
        <th>
            Price
        </th>
        <th>
            Book Name
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Invoice.in_id)
        </th>*@
        <th>
            Delivery to
        </th>
        <th>
            Address
        </th>
        <th>
            Status
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Transaction_bill)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transaction_qty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transaction_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transaction_price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_Books.Book_name)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Invoice.in_id)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.tbl_member.mem_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbl_member.mem_address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.transaction_status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Transaction_id }) @*|
            @Html.ActionLink("Details", "Details", new { id = item.Transaction_id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Transaction_id })*@
        </td>
    </tr>
}

</table>
