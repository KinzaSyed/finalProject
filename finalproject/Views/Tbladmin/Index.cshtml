@model IEnumerable<finalproject.Models.Tbl_admin>

@{
    ViewBag.Title = "Index";
}

<h2>All Admins</h2>

<a class="btn btn-info btn-lg" href="@Url.Action("Create")">Add Admin</a>
<br  />
<br />

<table  class="table table-striped table-responsive">
    <tr>
        <th>
            Name
        </th>
        <th>
            Email
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Admin_password)
            </th>*@
        <th>
            Contact
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Admin_status)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_email)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.Admin_password)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Admin_contact)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.Admin_status)
                </td>*@
            <td>

                <a class="btn btn-success btn-sm" href="@Url.Action("Edit", new { id = item.Admin_id })">Edit</a>
                <a class="btn btn-danger btn-sm" href="@Url.Action("Delete", new { id = item.Admin_id })">Delete</a>


                <a class="btn btn-info btn-sm" href="@Url.Action("Details", new { id = item.Admin_id })">Details</a>





            
            </td>
        </tr>
    }

</table>
